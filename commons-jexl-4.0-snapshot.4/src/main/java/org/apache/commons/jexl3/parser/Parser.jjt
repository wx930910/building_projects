{
    ( <FINAL> { isFinal = true; } )?
    ( <VAR> ( <and> { isRequired = true; } )?
      | <INT> { type = Integer.TYPE; }
      | <LONG> { type = Long.TYPE; }
      | <SHORT> { type = Short.TYPE; }
      | <BYTE> { type = Byte.TYPE; }
      | <CHAR> { type = Character.TYPE; }
      | <BOOLEAN> { type = Boolean.TYPE; }
